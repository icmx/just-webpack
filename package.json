{
  "name": "just-webpack",
  "version": "0.1.7",
  "private": true,
  "description": "Bare minimum boilerplate featuring Webpack, basic EJS templating and PostCSS styling, all live-reloadable. Code linting and formatting are provided by ESLint and Prettier",
  "scripts": {
    "watch": "webpack serve --config-name watch",
    "pretty": "prettier --write src",
    "lint": "eslint src",
    "build": "webpack --config-name build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icmx/just-webpack.git"
  },
  "keywords": [
    "boilerplate",
    "minimal",
    "webpack",
    "postcss",
    "live-reload"
  ],
  "author": "icmx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icmx/just-webpack/issues"
  },
  "homepage": "https://github.com/icmx/just-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-csso": "^6.0.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
