{
  "name": "just-webpack",
  "version": "0.1.5",
  "private": true,
  "description": "Bare minimum boilerplate featuring Webpack, basic EJS templating and PostCSS styling, all live-reloadable. Code linting and formatting are provided by ESLint and Prettier",
  "scripts": {
    "start": "webpack serve --config-name serve",
    "pretty": "prettier --write src",
    "lint": "eslint src",
    "build": "webpack --config-name build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icmx/just-webpack.git"
  },
  "keywords": [
    "boilerplate",
    "minimal",
    "webpack",
    "postcss",
    "live-reload"
  ],
  "author": "icmx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icmx/just-webpack/issues"
  },
  "homepage": "https://github.com/icmx/just-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.2",
    "postcss-csso": "^5.0.1",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.1",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
